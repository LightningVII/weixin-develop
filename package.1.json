{
  "name": "rxjs-todomvc",
  "version": "0.1.0",
  "description": "TODO-MVC implement by RxJS",
  "main": "app.js",
  "scripts": {
    "start": "node --harmony app.js",
    "src-start": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "src-test": "echo \"Error: no test specified\" && exit 1",
    "start:prod": "cross-env NODE_ENV=production npm start",
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:server": "cross-env NODE_ENV=development nodemon --debug --watch app --watch conf --watch config app.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --progress --config ./build/webpack.client.config.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --progress --hide-modules --config ./build/webpack.client.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --progress --hide-modules --config ./build/webpack.server.config.js",
    "test": "npm run test:unit:egg && npm run test:unit:vue",
    "test:e2e": "node test/e2e/runner.js",
    "test:unit:egg": "cross-env VUE_ENV=server TESTS=test/egg/**/*.test.js egg-bin test -r intelli-espower-loader",
    "test:unit:vue": "cross-env BABEL_ENV=development karma start test/vue/karma.unit.config.js",
    "test:cover": "npm run test:cover:egg && npm run test:cover:vue",
    "test:cover:egg": "egg-bin cov -r intelli-espower-loader",
    "test:cover:vue": "cross-env BABEL_ENV=development karma start test/vue/karma.cover.config.js",
    "lint": "npm run lint:egg && npm run lint:vue",
    "lint:egg": "eslint --ext js app/** --fix",
    "lint:vue": "",
    "release": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major"
  },
  "keywords": [
    "RxJS",
    "Todo"
  ],
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "dependencies": {
    "apollo-client": "^0.5.12",
    "apollo-server": "^0.3.3",
    "axios": "^0.15.2",
    "bootstrap": "^3.3.7",
    "co": "^4.6.0",
    "co-sqlite3": "^0.1.3",
    "element-ui": "^1.0.5",
    "es6-promise": "^4.0.5",
    "graphql": "^0.8.2",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "koa": "^2.0.0-alpha.7",
    "koa-body": "^1.6.0",
    "koa-router": "^5.4.0",
    "muse-ui": "^2.0.0-alpha.8",
    "rxjs": "^5.0.0-rc.4",
    "swig": "^1.4.2",
    "vue": "^2.1.3",
    "vue-router": "^2.0.1",
    "vue-rx": "^2.2.0",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^3.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.33",
    "@types/chai": "^3.4.34",
    "@types/jquery": "^2.0.33",
    "@types/node": "^6.0.46",
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-component": "^0.6.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.2",
    "npm-install-webpack-plugin": "^4.0.4",
    "npm-run-all": "^3.1.2",
    "postcss-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^0.9.5",
    "typescript": "^2.0.10",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^9.9.5",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0"
  }
}
